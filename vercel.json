{
    "version": 2,
    "name": "python-wsgi-app",
    "builds": [{
        "src": "vercel_app/wsgi.py",
        "use": "@ardnt/now-python-wsgi",
        "config": {"runtime": "python3.9"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "vercel_app/wsgi.py"
        }
    ]
}
